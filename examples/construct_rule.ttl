@prefix kees: <http://linkeddata.center/kees/v1#> .

[] kees:ruleText """
		PREFIX gr: <http://purl.org/goodrelations/v1#>
		PREFIX : <urn:test:>
		IF {
			GRAPH ?g {
				?offering gr:hasPriceSpecification ?ps .
				?ps gr:hasCurrencyValue ?price .
				FILTER ( ?price >= 200.00 )
			}
		}
		THEN {
			?offering a :ExpensiveProduct .
		}
""" .