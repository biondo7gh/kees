@prefix kees: <http://linkeddata.center/kees/v1#> .
[]  a kees:SparqlIngestion ;
	kees:queryName <http://dbpedia.org/sparql> ;
	kees:sparqlEndpoint <http://dbpedia.org/sparql> ;
	kees:constructQuery """
		CONSTRUCT { ?s ?p ?o } 
		WHERE { ?s ?p ?o }
	""" ;
	kees:pageSize 1000 ;
	kees:fromPage 1 ;
	kees:toPage 1 ;
	kees:queryMethod "POST" ;
	kees:onFetchingError [ kees:hasResilience 0 ] ;
	kees:updateFrequency [ kees:hasFrequencyPeriod 3600 ] ;
.