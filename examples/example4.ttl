@prefix void: <http://rdfs.org/ns/void#> .
@prefix kees: <http://linkeddata.center/kees/v1#> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .

<urn:kees:kb>
	void:subset 
		[ 
			kees:source <http://dbpedia.org#first_10_triples> ;
			kees:accrualPeriodicity kees:monthly ;
			kees:onFetchingError kees:keepDataset ;
			kees:accrualPolicy [ a kees:SparqlIngestion ;
				kees:useSparqlService [ sd:endpoint <http://dbpedia.org/sparql> ] ;
				kees:constructQuery """
					CONSTRUCT {
						?s ?p ?o
					}
					WHERE {
						?s ?p ?o
					}
				""" ;
				kees:pageSize 10 ;
				kees:fromPage 1 ;
				kees:toPage 1 ;
				kees:queryMethod "POST" ;
			]
		] ;
    .
