@prefix kees: <http://linkeddata.center/kees/v1#> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix self: <http://linkeddata.center/a/here_your_grap_db_name#> .

self:kb a kees:KnowledgeBase ;
	kees:hasConfigGraph [ a kees:KBConfigurationGraph ; sd:name self:config];
	kees:hasTBoxGraph [ a kees:TBoxGraph ; sd:name self:tbox]; 
	kees:hasInferenceGraph [ a kees:InferredKnowledgeGraph ; sd:name self:inferences ];
	kees:hasABoxGraph [
		a kees:ABoxGraph ;
		sd:name <http://demo.hub1.linkeddata.center.s3.amazonaws.com/data/cities.rdf> ;
		kees:accrualPolicy [
			a kees:BulkIngestion ;
			kees:source <http://demo.hub1.linkeddata.center.s3.amazonaws.com/data/cities.rdf>;
			kees:onFetchingError [ a kees:RetentionPolicy ; kees:hasResilience 0 ] ;
			kees:updateFrequency [ a kees:Frequency ; kees:hasFrequencyPeriod 3600 ] ;
		]
	] , [
		a kees:ABoxGraph ;
		sd:name <https://demo.hub1.linkeddata.center.s3.amazonaws.com/data/countries.rdf> ;
		kees:accrualPolicy [
			a kees:BulkIngestion ;
			kees:source <https://demo.hub1.linkeddata.center.s3.amazonaws.com/data/countries.rdf>;
			kees:onFetchingError [ a kees:RetentionPolicy ; kees:hasResilience 0 ] ;
			kees:updateFrequency [ a kees:Frequency ; kees:hasFrequencyPeriod 3600 ] ;
		]
	] ;
.