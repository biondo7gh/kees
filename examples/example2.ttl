@prefix kees: <http://linkeddata.center/kees/v1#> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix example: <http://demo.hub1.linkeddata.center.s3.amazonaws.com/data/> .
@prefix self: <http://linkeddata.center/a/here_your_grap_db_name#>

[] owl:sameAs self:kb ;
  kees:factSubset [
    a kees:ABoxGraph ;
    kees:accrualPolicy kees:httpGetPagedContent ;
    kees:source examplecities.rdf
  ], [
    a kees:ABoxGraph ;
    kees:accrualPolicy kees:httpGetPagedContent ;
    kees:source examplecountries.rdf
  ] .
  
self:kb
  a kees:KnowledgeBase ;
  void:subset self:config , self:tbox , self:inferences ;
  dc:identifier 0 .

self:config a kees:KBConfigurationGraph ; sd:name self:config .
self:tbox a kees:TBoxGraph ; sd:name self:tbox .
self:inferences a kees:InferredKnowledgeGraph ; sd:name self:inferences .
