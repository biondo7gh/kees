@prefix kees: <http://linkeddata.center/kees/v1#> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix example: <http://demo.hub1.linkeddata.center.s3.amazonaws.com/data/> .
@prefix : <http://linkeddata.center/a/here_your_grap_db_name#> .

[] owl:sameAs :kb ;
  kees:factSubset [
    a kees:ABoxGraph ;
    kees:accrualPolicy kees:httpGetPagedContent ;
    kees:source example:cities.rdf
  ], [
    a kees:ABoxGraph ;
    kees:accrualPolicy kees:httpGetPagedContent ;
    kees:source example:countries.rdf
  ] .
  
:kb
  a kees:KnowledgeBase ;
  void:subset :config , :tbox , :inferences ;
  dc:identifier 0 .

:config a kees:KBConfigurationGraph ; sd:name :config .
:tbox a kees:TBoxGraph ; sd:name :tbox .
:inferences a kees:InferredKnowledgeGraph ; sd:name :inferences .
