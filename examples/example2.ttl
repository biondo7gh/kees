@prefix void: <http://rdfs.org/ns/void#> .
@prefix sd: <http://www.w3.org/ns/sparql-service-description#> .
@prefix kees: <http://linkeddata.center/kees/v1#> .

<urn:kees:kb> a void:Dataset, sd:Dataset, kees:Knowledgebase ;
	void:subset _:dataset1, _:dataset2
.

_:dataset1 a sd:NamedGraph,  kees:ABoxGraph;
	sd:name <http://data.example.com/file1.ttl> ;
	kees:source <http://data.example.com/file1.ttl> ;
	kees:accrualPolicy _:simpleBulkIngestion 
.

_:dataset2 a sd:NamedGraph, kees:ABoxGraph;
	sd:name <http://data.example.com/file2.ttl> ;
	kees:source <http://data.example.com/file2.ttl> ;
	kees:accrualPolicy _:simpleBulkIngestion 
.
		
_:simpleBulkIngestion a kees:AccrualPolicy, kees:BulkIngestion ;
	kees:updateFrequency _:hourly ;
	kees:onFetchingError _:dropDataset ;
	kees:forceAccept "text/turtle"
.

_:hourly a kees:Frequency ;
	kees:hasFrequencyPeriod 3600
.

_:dropDataset a kees:RetentionPolicy ;
	kees:hasResilience 0 
.