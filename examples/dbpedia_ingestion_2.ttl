@prefix kees: <http://linkeddata.center/kees/v1#> .
[]  a kees:SparqlIngestion
	kees:queryName <urn:citynames> ;
	kees:sparqlEndpoint [ sd:endpoint <http://dbpedia.org/sparql> ] ;
	kees:constructQuery """
		PREFIX yago: <http://dbpedia.org/class/yago/>
		CONSTRUCT {
			?s <urn:city:name> ?label
		} WHERE {
			?s a yago:Municipality108626283 ; rdfs:label ?label
			FILTER(LANGMATCHES(LANG(?label), 'en'))
		}
	""" ;
	kees:pageSize 50000 ;
	kees:fromPage 1 ;
	kees:toPage 4 ;
	kees:queryMethod "POST" ;
	kees:onFetchingError [ kees:hasResilience 4 ] ;
	kees:updateFrequency [ kees:hasFrequencyPeriod 604800 ] ;
.