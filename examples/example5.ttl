@prefix kees: <http://linkeddata.center/kees/v1#> .
[]  
	kees:useGraphName <http://dbpedia.org/sparql#1_200K_dump>
	kees:useSparqlService [ sd:endpoint <http://dbpedia.org/sparql> ] ;
	kees:constructQuery """
		CONSTRUCT {
			?s ?p ?o
		} WHERE {
			?s ?p ?o
		}
	""" ;
	kees:pageSize 50000 ;
	kees:fromPage 1 ;
	kees:toPage 4 ;
	kees:queryMethod "POST" ;
	kees:onFetchingError [ kees:hasResilience 4 ] ;
	kees:updateFrequency [ kees:hasFrequencyPeriod 604800 ] ;
.