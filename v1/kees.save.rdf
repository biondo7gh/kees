    <!-- http://linkeddata.center/kees/v1#AccrualPolicy -->

    <owl:Class rdf:about="http://linkeddata.center/kees/v1#AccrualPolicy">
        <rdfs:comment xml:lang="en">A KEES agent MUST recognize kees:modify and kees:create individuals as valid object for dct:accrualPolicy property in a kees:Plan. In case of inconsistencies or if no dct:accrualPolicy property is specified, the agent MUST choose kees:create.

If the accrual policy is kees:create, the named graph and all related metadata MUST be deleted and recreated BEFORE to execute accrual policies.</rdfs:comment>
        <rdfs:label xml:lang="en">AccrualPolicy</rdfs:label>
    </owl:Class>
    


    <!-- http://linkeddata.center/kees/v1#Agent -->

    <owl:Class rdf:about="http://linkeddata.center/kees/v1#Agent">
        <rdfs:label xml:lang="en">Agent</rdfs:label>
        <rdfs:label xml:lang="en">Is a software process that understands a portion the KEES language profile and that it is able to do actions on a knowledge base. For instance, it could be able to ingest data and/or to answer some questions.</rdfs:label>
    </owl:Class>
    


    <!-- http://linkeddata.center/kees/v1#IngestionPlan -->

    <owl:Class rdf:about="http://linkeddata.center/kees/v1#IngestionPlan">
        <rdfs:subClassOf rdf:resource="http://linkeddata.center/kees/v1#Plan"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://linkeddata.center/kees/v1#dataSource"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://linkeddata.center/kees/v1#ReasoningPlan"/>
        <rdfs:comment xml:lang="en">Is a specialization of prov:Plan class. Describes how to create a graph in the knowledge base extracting facts from a data source.</rdfs:comment>
        <rdfs:label xml:lang="en">IngestionPlan</rdfs:label>
    </owl:Class>
    


    <!-- http://linkeddata.center/kees/v1#KnowledgeBase -->

    <owl:Class rdf:about="http://linkeddata.center/kees/v1#KnowledgeBase">
        <rdfs:comment xml:lang="en">A container of linked data with a purpose, that is a related set information that can be composed to provide answer to some questions.</rdfs:comment>
        <rdfs:label xml:lang="en">KnowledgeBase</rdfs:label>
    </owl:Class>
    


    <!-- http://linkeddata.center/kees/v1#KnowledgeBaseDescription -->

    <owl:Class rdf:about="http://linkeddata.center/kees/v1#KnowledgeBaseDescription">
        <rdfs:comment xml:lang="en">A document describing a knowledge base configuration. A KEES Agent should be able to rebuild the whole knowlege graph just looking to the KEES configuration.</rdfs:comment>
        <rdfs:label xml:lang="en">KnowledgeBaseDescrtiption</rdfs:label>
    </owl:Class>
    


    <!-- http://linkeddata.center/kees/v1#Plan -->

    <owl:Class rdf:about="http://linkeddata.center/kees/v1#Plan">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://linkeddata.center/kees/v1#builds"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A subclass of a prov:Plan. States a named graph accual activity plan</rdfs:comment>
        <rdfs:label xml:lang="en">plan</rdfs:label>
    </owl:Class>
    


    <!-- http://linkeddata.center/kees/v1#Question -->

    <owl:Class rdf:about="http://linkeddata.center/kees/v1#Question">
        <rdfs:comment xml:lang="en">The Question represents the reason for the the knowledge base existence. In other words, the knoledge base exists to answer to questions. Question are natural language expressions that can be expressed as parametric SPARQL queries on a populated knowledge graph. The answer to a question can be a table of data, a structured document, a boolean or a translation of these in a natural language sentences.</rdfs:comment>
        <rdfs:label xml:lang="en">Question</rdfs:label>
    </owl:Class>
    


    <!-- http://linkeddata.center/kees/v1#ReasoningPlan -->

    <owl:Class rdf:about="http://linkeddata.center/kees/v1#ReasoningPlan">
        <rdfs:subClassOf rdf:resource="http://linkeddata.center/kees/v1#Plan"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://linkeddata.center/kees/v1#axiom"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange>
                    <rdfs:Datatype>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#anyURI"/>
                            <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#string"/>
                        </owl:unionOf>
                    </rdfs:Datatype>
                </owl:onDataRange>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Describes how to materialize new information from existing knowledge base facts.</rdfs:comment>
        <rdfs:label xml:lang="en">ReasoningPlan</rdfs:label>
    </owl:Class>
    


