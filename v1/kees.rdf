    <!-- http://linkeddata.center/kees/v1#hasABoxGraph -->

    <owl:ObjectProperty rdf:about="http://linkeddata.center/kees/v1#hasABoxGraph">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://linkeddata.center/kees/v1#KnowledgeBase"/>
        <rdfs:range rdf:resource="http://linkeddata.center/kees/v1#ABoxGraph"/>
        <rdfs:comment xml:lang="en">The related resource is one of the ABox graphs that contain linked data facts in the described  knowlege base.</rdfs:comment>
        <rdfs:label xml:lang="en">has ABox graph</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasAccrualPolicy -->

    <owl:ObjectProperty rdf:about="http://linkeddata.center/kees/v1#hasAccrualPolicy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://linkeddata.center/kees/v1#KnowledgeBase"/>
        <rdfs:range rdf:resource="http://linkeddata.center/kees/v1#KnowledgeAccrualPolicy"/>
        <rdfs:comment xml:lang="en">The related resource is a policy governing the addition of a named graph to the described knowledge base. Very similar to the dct:accrualPolicy property.</rdfs:comment>
        <rdfs:label xml:lang="en">has accrual policy</rdfs:label>
    </owl:ObjectProperty>
    


