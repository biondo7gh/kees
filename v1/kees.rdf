     -->

    


    <!-- http://purl.org/dc/elements/1.1/contributor -->

    <owl:AnnotationProperty rdf:about="&dc;contributor"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:AnnotationProperty rdf:about="&dc;description"/>
    


    <!-- http://purl.org/dc/elements/1.1/modifed -->

    <owl:AnnotationProperty rdf:about="&dc;modifed"/>
    


    <!-- http://purl.org/dc/elements/1.1/rights -->

    <owl:AnnotationProperty rdf:about="&dc;rights"/>
    


    <!-- http://purl.org/dc/elements/1.1/subject -->

    <owl:AnnotationProperty rdf:about="&dc;subject"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="&dc;title"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="&dcterms;license"/>
    


    <!-- http://www.w3.org/2002/07/owl#deprecated -->

    <owl:AnnotationProperty rdf:about="&owl;deprecated"/>
    


    <!-- http://xmlns.com/foaf/0.1/homepage -->

    <owl:AnnotationProperty rdf:about="&foaf;homepage"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#success -->

    <rdf:Description rdf:about="&xsd;success">
        <rdfs:label xml:lang="en">has input</rdfs:label>
        <owl:priorVersion xml:lang="en">task  processor input as a json structure.</owl:priorVersion>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linkeddata.center/kees/v1#accrualPolicy -->

    <owl:ObjectProperty rdf:about="&kees;accrualPolicy">
        <rdfs:label xml:lang="en">accrual policy</rdfs:label>
        <rdfs:comment xml:lang="en">The policy governing the addition of items to a collection.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;ABoxGraph"/>
        <rdfs:range rdf:resource="&kees;AccrualPolicy"/>
        <rdfs:domain rdf:resource="&kees;TBoxGraph"/>
        <rdfs:seeAlso rdf:resource="&dcterms;accrualPolicy"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasABoxGraph -->

    <owl:ObjectProperty rdf:about="&kees;hasABoxGraph">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">a knowlege base  can have 0 or more ABox graph that contains facts.</rdfs:comment>
        <rdfs:range rdf:resource="&kees;ABoxGraph"/>
        <rdfs:domain rdf:resource="&kees;KnowledgeBase"/>
        <rdfs:subPropertyOf rdf:resource="&sd;namedGraph"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasConfigGraph -->

    <owl:ObjectProperty rdf:about="&kees;hasConfigGraph">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">A knowledge base is caracterized by a config  graph, by default named self:config.</rdfs:comment>
        <rdfs:range rdf:resource="&kees;KBConfigGraph"/>
        <rdfs:domain rdf:resource="&kees;KnowledgeBase"/>
        <rdfs:subPropertyOf rdf:resource="&sd;namedGraph"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasInferenceGraph -->

    <owl:ObjectProperty rdf:about="&kees;hasInferenceGraph">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">a Knowledge base can ave a graph where are placed all materialized triples.</rdfs:comment>
        <rdfs:range rdf:resource="&kees;InferredKnowledgeGraph"/>
        <rdfs:domain rdf:resource="&kees;KnowledgeBase"/>
        <rdfs:subPropertyOf rdf:resource="&sd;namedGraph"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasIngestionActivityReportGraph -->

    <owl:ObjectProperty rdf:about="&kees;hasIngestionActivityReportGraph">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">A knowlegde base can have  some activity report graph that store facts about an kees cycle executions.</rdfs:comment>
        <rdfs:range rdf:resource="&kees;IngestionActivityReportGraph"/>
        <rdfs:domain rdf:resource="&kees;KnowledgeBase"/>
        <rdfs:subPropertyOf rdf:resource="&sd;namedGraph"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasTBoxGraph -->

    <owl:ObjectProperty rdf:about="&kees;hasTBoxGraph">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:comment xml:lang="en">A knowledge base can have a TBox graph where to store vocabularies and taxonomies.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;KnowledgeBase"/>
        <rdfs:range rdf:resource="&kees;TBoxGraph"/>
        <rdfs:subPropertyOf rdf:resource="&sd;namedGraph"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasTargetGraph -->

    <owl:ObjectProperty rdf:about="&kees;hasTargetGraph">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdf:type rdf:resource="&owl;InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="&kees;AccrualPolicy"/>
        <rdfs:range rdf:resource="&sd;NamedGraph"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#onFetchingError -->

    <owl:ObjectProperty rdf:about="&kees;onFetchingError">
        <rdfs:label xml:lang="en">on fetching error</rdfs:label>
        <rdfs:comment xml:lang="en">Allows to declare the policy to adopt when an error occurs during resource 
fetching- If not present kees:keepDataset is inferred.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;AccrualPolicy"/>
        <rdfs:range rdf:resource="&kees;RetentionPolicy"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#requiresAuthentication -->

    <owl:ObjectProperty rdf:about="&kees;requiresAuthentication">
        <rdfs:label xml:lang="en">requires authentication</rdfs:label>
        <rdfs:comment xml:lang="en">use an authentication method, if not present no authentication required.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;AccrualPolicy"/>
        <rdfs:range rdf:resource="&kees;AuthenticationMethod"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#updateFrequency -->

    <owl:ObjectProperty rdf:about="&kees;updateFrequency">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">update frequency</rdfs:label>
        <rdfs:comment xml:lang="en">The a time frame after witch a source is supposed  changes.  If not specified , one hour is guessed .</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;AccrualPolicy"/>
        <rdfs:range rdf:resource="&kees;Frequency"/>
        <rdfs:seeAlso rdf:resource="&dcterms;accrualPeriodicity"/>
    </owl:ObjectProperty>
    


    <!-- http://linkeddata.center/kees/v1#usesSparqlService -->

    <owl:ObjectProperty rdf:about="&kees;usesSparqlService">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">uses SPARQL service</rdfs:label>
        <rdfs:comment xml:lang="en">The ingestion is based on a query to the object sparql service.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;SparqlIngestion"/>
        <rdfs:range rdf:resource="&sd;Service"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/sparql-service-description#namedGraph -->

    <owl:ObjectProperty rdf:about="&sd;namedGraph"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linkeddata.center/kees/v1#constructQuery -->

    <owl:DatatypeProperty rdf:about="&kees;constructQuery">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">construct query</rdfs:label>
        <rdfs:comment xml:lang="en">The SPARQL 1.1.  query to extract data from a SPARQL endpoint. No paging constructs allowed ( OFSET and LIMIT ), use pageSize, fromPage and toPage instead.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;SparqlIngestion"/>
        <rdfs:range rdf:resource="&xsd;success"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#executionOrder -->

    <owl:DatatypeProperty rdf:about="&kees;executionOrder">
        <rdfs:label xml:lang="en">rule execution order</rdfs:label>
        <rdfs:comment xml:lang="en">An optiona integer that determine the ordere of execution of a rule ( ascending). If not specified same meaning as 0 is expected.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;Rule"/>
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#forceAccept -->

    <owl:DatatypeProperty rdf:about="&kees;forceAccept">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">force accept</rdfs:label>
        <rdfs:comment xml:lang="en">Set specified http accept header in requesting source.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;AccrualPolicy"/>
        <rdfs:range rdf:resource="&xsd;success"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#fromPage -->

    <owl:DatatypeProperty rdf:about="&kees;fromPage">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">from page</rdfs:label>
        <rdfs:comment xml:lang="en">start returning page  number page in a query iteration. Defaults to 1 if not present.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;SparqlIngestion"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#hadETag -->

    <owl:DatatypeProperty rdf:about="&kees;hadETag">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">had ETag</rdfs:label>
        <rdfs:comment xml:lang="en">States that the subject  graph was derived  from a resource that had the referred http header ETag when retrived during  accrual task.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;ABoxGraph"/>
        <rdfs:range rdf:resource="&xsd;success"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#hadLastModified -->

    <owl:DatatypeProperty rdf:about="&kees;hadLastModified">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">had Last Modified</rdfs:label>
        <rdfs:comment xml:lang="en">the Last Modified date returned when the data source  was accrued to the knowledge base.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;ABoxGraph"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasFrequencyPeriod -->

    <owl:DatatypeProperty rdf:about="&kees;hasFrequencyPeriod">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has frequency period</rdfs:label>
        <rdfs:comment xml:lang="en">Frequency period in seconds.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;Frequency"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
        <rdfs:seeAlso rdf:resource="http://www.w3.org/TR/swbp-xsch-datatypes/#section-duration"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasResilience -->

    <owl:DatatypeProperty rdf:about="&kees;hasResilience">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has resilience</rdfs:label>
        <rdfs:comment xml:lang="en">The number of times  ABOX graph is keep in storage after a failure in fetchin it. The default is 0 that means that when a graph fails to load it is removed from the knowledge base. If the value of this attribute is, for instance 3, this means that the Graph will be keept in the knowledge base untill its loading fails for three consecutive  times (the fourth error drives a graph drop). A succesfull loading reset the fail counter.N.B. a &quot;not modified&quot; message does not reset the fail counter.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;RetentionPolicy"/>
        <rdfs:range rdf:resource="&xsd;nonNegativeInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasTargetGraphName -->

    <owl:DatatypeProperty rdf:about="&kees;hasTargetGraphName">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has target graph name</rdfs:label>
        <rdfs:comment xml:lang="en">This attribute states that the accrual policy subject  is about the creation/maintenance of a graph named as the referenced object.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;AccrualPolicy"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#hasTargetGraphType -->

    <owl:DatatypeProperty rdf:about="&kees;hasTargetGraphType">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#includes -->

    <owl:DatatypeProperty rdf:about="&kees;includes">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">include</rdfs:label>
        <rdfs:comment xml:lang="en">the URL of a web resource exposing a Kees configuration data.  It is a shortcut for kees:hasTargetGraph [ a kees:KBConfigGraph; sd:name &lt;configSource&gt; ].</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;ConfigIngestion"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#pageSize -->

    <owl:DatatypeProperty rdf:about="&kees;pageSize">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">page size</rdfs:label>
        <rdfs:comment xml:lang="en">The size for a SPARQL querty. Equivalent  with LIMIT clause. Defaults to 1000 if not defined</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;SparqlIngestion"/>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#password -->

    <owl:DatatypeProperty rdf:about="&kees;password">
        <rdfs:label xml:lang="en">password</rdfs:label>
        <rdfs:comment xml:lang="en">password in basic autentication method</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;HttpBasicAuthentication"/>
        <rdfs:range rdf:resource="&xsd;success"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#queryMethod -->

    <owl:DatatypeProperty rdf:about="&kees;queryMethod">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">query method</rdfs:label>
        <rdfs:comment xml:lang="en">The http request method used in a SPARQL query. Accepted metods: &quot;POST&quot;  and &quot;GET&quot;. Defaults to &quot;POST&quot; if not specified. </rdfs:comment>
        <rdfs:domain rdf:resource="&kees;SparqlIngestion"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:oneOf>
                    <rdf:Description>
                        <rdf:type rdf:resource="&rdf;List"/>
                        <rdf:first>GET</rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="&rdf;List"/>
                                <rdf:first>POST</rdf:first>
                                <rdf:rest rdf:resource="&rdf;nil"/>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </owl:oneOf>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#ruleText -->

    <owl:DatatypeProperty rdf:about="&kees;ruleText">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment xml:lang="en">The roule text body in the form:
prefixes declaration
IF { sparql where clause }
THEN { sparql insert(delete clause }.

Example:
PREFIX xsd: &lt;http://www.w3.org/2001/XMLSchema#&gt;
PREFIX gr: &lt;http://purl.org/goodrelations/v1#&gt;
PREFIX :&lt;urn:test:&gt;
IF {
           ?offering gr:hasPriceSpecification ?ps .
           ?ps gr:hasCurrencyValue ?price .
           FILTER (?price &gt;= 200.00).
}
THEN {
           ?offering a :ExpensiveProduct .
}

</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;Rule"/>
        <rdfs:range rdf:resource="&xsd;success"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#source -->

    <owl:DatatypeProperty rdf:about="&kees;source">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">source</rdfs:label>
        <rdfs:comment xml:lang="en">the URL of a web resource exposing RDF data.  It is a shortcut for kees:hasTargetGraph [ a kees:LinkedDataGraph; sd:name &lt;source&gt; }</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;LinkedDataResourceIngestion"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#supportsHttpCahing -->

    <owl:DatatypeProperty rdf:about="&kees;supportsHttpCahing">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">supports http caching</rdfs:label>
        <rdfs:comment xml:lang="en">support http caching specification (RFC 2616 ) managing sending HTTP requests with proper If-Modified-Since and If-None-Match headers. If not present  false is inferred.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;BulkIngestion"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#supportsLDPPP -->

    <owl:DatatypeProperty rdf:about="&kees;supportsLDPPP">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">supports LDPPP</rdfs:label>
        <rdfs:comment xml:lang="en">the ingestion policy support Linked Data Platform Paging Protocol following link=next in http headers. If not specified false is inferred false.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;BulkIngestion"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#toPage -->

    <owl:DatatypeProperty rdf:about="&kees;toPage">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">to page</rdfs:label>
        <rdfs:comment xml:lang="en">the page upper limit in a SPARQL query. if not present defaults to last available page in the dataset.</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;SparqlIngestion"/>
        <rdfs:range rdf:resource="&xsd;positiveInteger"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#username -->

    <owl:DatatypeProperty rdf:about="&kees;username">
        <rdfs:label xml:lang="en">username</rdfs:label>
        <rdfs:comment xml:lang="en">username in basic http autentication method</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;HttpBasicAuthentication"/>
        <rdfs:range rdf:resource="&xsd;success"/>
    </owl:DatatypeProperty>
    


    <!-- http://linkeddata.center/kees/v1#vocabulary -->

    <owl:DatatypeProperty rdf:about="&kees;vocabulary">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">vocabulary</rdfs:label>
        <rdfs:comment xml:lang="en">It&apos;s a shortcut for kees:hasTargetGraph [ a TBoxGraph; sd:name &lt;vocabulary&gt; ]</rdfs:comment>
        <rdfs:domain rdf:resource="&kees;TBoxIngestion"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linkeddata.center/kees/v1#ABoxGraph -->

    <owl:Class rdf:about="&kees;ABoxGraph">
        <rdfs:label xml:lang="en">ABox Graph</rdfs:label>
        <rdfs:subClassOf rdf:resource="&sd;NamedGraph"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&kees;hadLastModified"/>
